{
  "name": "TwitchExtension+",
  "version": "0",
  "description": "Retrouve toutes les informations concernant tes streamers favoris !",
  "author": "Quentin L.",
  "manifest_version": 3,
  "permissions": [],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_popup": "src/html/popup.html",
    "default_icon": {
      "16": "/images/icon16.png",
      "48": "/images/icon48.png",
      "128": "/images/icon128.png"
    }
  }
}

